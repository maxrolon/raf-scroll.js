{
  "name": "raf-scroll.js",
  "version": "0.0.5",
  "description": "Performant scroll events with request animation frame",
  "author": "maxrolon",
  "main": "./src/index.js",
  "scripts": {
    "browser": "mkdir -p browser && browserify src/index.js --standalone src/index.js > browser/index.js",
    "watch": "watchify test/dev.js -v -d -o test/index.js",
    "test": "ava test/test.js -v",
    "size": "bundle-size ./browser/index.js"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "ava": {
    "require": [
      "./test/test-helpers.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^12.0.1",
    "bundle-size": "^0.7.0",
    "jsdom": "^9.5.0",
    "minifyify": "^7.3.3",
    "watchify": "^3.7.0"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/maxrolon/raf-scroll.js.git"
  },
  "license": "MIT"
}
